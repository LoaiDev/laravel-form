{
    "name": "loaidev/laravel-form",
    "description": "create forms and accept client entries in laravel",
    "keywords": [
        "loaidev",
        "form",
        "laravel"
    ],
    "homepage": "https://github.com/loaidev/laravel-form",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Loai Akram",
            "email": "lolyakram11@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "LoaiDev\\Form\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LoaiDev\\Form\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LoaiDev\\Form\\FormServiceProvider"
            ]
        }
    }
}
